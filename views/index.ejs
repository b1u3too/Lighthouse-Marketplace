<!DOCTYPE html>
<html lang="en">

<head>
  <title>Home Page</title>

  <%- include('partials/head'); %>
</head>

<body>

  <header>
    <%- include('partials/nav')%>
  </header>

  <section class="listing-tiles">
    <% for (let item of items) { %>
      <article class="tile">
        <a href="/api/items/<%=item.id%>"><%= item.id %></a>
        <p> <%= item.name %></p>
      </article>
    <% } %>
  </section>
</body>

</html>
