<!DOCTYPE html>
<html lang="en">

<head>
  <title>My messages</title>
  <%- include('partials/head'); %>
</head>

<body>
  <header>
    <%- include('partials/nav')%>
  </header>

  <h2>My messages</h2>

  <section class="my-listing-feed">
    <% for (let message of messages) { %>
      <article>
        <div>
          <a href='/api/messages/<%= message.id %>'> My messages for item <%= message.id %> </a>
        </div>
      </article>
      <% } %>
  </section>

  <div>
    <a href="/api/messages/new">Create new message</a>
  </div>

  <footer>
    <%- include('partials/footer')%>
  </footer>
</body>

</html>
